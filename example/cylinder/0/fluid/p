FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/fluid";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 1e5;

boundaryField
{
    "(\w*X|\w*Y|\w*Z)"
    {
	    type		calculated;
	    value		uniform 1e5;
    } 

    "(\w*_fluid)"
    {
        type        calculated;
	    value		uniform 1e5;
    }
}


