FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/solid";
    object      elpot;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 2 -3 0 0 -1 0];

internalField   uniform 0;

boundaryField
{
    posTerm_solid
    {
        type        fixedValue;
        value       uniform 0.17;  
    }

    negTerm_solid
    {
        type        fixedValue;
        value       uniform 0;
    }

    cylSurf_solid
    {
        type        zeroGradient;
    }
}


